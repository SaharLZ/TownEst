{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/pages/Projects.js", "app/Slider.js", "app/components/Lottie.js", "app/HelloArchitect/Projects/page.js", "app/HelloArchitect/Events/page.js", "app/HelloArchitect/Events/AddEventModal.js", "app/Login/page.js", "app/SignUp/page.js", "app/HelloArchitect/component/animatedbg.js"],
  "exclude": ["node_modules"]
}
